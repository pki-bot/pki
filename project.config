[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group test-pki-approvers
	owner = group user/pki-jenkins-bot
[access "refs/for/refs/*"]
	read = group test-pki-approvers
	read = group user/pki-jenkins-bot
	push = group test-pki-approvers
	push = group user/pki-jenkins-bot
	label-Code-Review = -2..+2 group test-pki-approvers
	label-Code-Review = -2..+2 group user/pki-jenkins-bot
	label-Verified = -1..+1 group test-pki-approvers
	label-Verified = -1..+1 group user/pki-jenkins-bot
	rebase = group test-pki-approvers
	rebase = group user/pki-jenkins-bot
	removeReviewer = group test-pki-approvers
	removeReviewer = group user/pki-jenkins-bot
	submit = group test-pki-approvers
	submit = group user/pki-jenkins-bot
[access "refs/heads/*"]
	read = group test-pki-approvers
	read = group user/pki-jenkins-bot
	create = group test-pki-approvers
	create = group user/pki-jenkins-bot
	push = group test-pki-approvers
	push = +force group user/pki-jenkins-bot
	pushMerge = group test-pki-approvers
	pushMerge = group user/pki-jenkins-bot
[access "refs/tags/*"]
	push = group test-pki-approvers
	push = group user/pki-jenkins-bot
	push = +force group user/pki-jenkins-bot
